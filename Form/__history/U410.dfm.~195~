  object qryCell: TADOQuery
    Connection = MainDm.MainDatabase
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT 1 AS SC_HOGI, SUM(TOTAL) AS TOTAL, SUM(CNT1) AS CNT1, SUM' +
        '(CNT2) AS CNT2, SUM(CNT3) AS CNT3, SUM(CNT4) AS CNT4, SUM(CNT5) ' +
        'AS CNT5'
      '       FROM'
      #9'   (SELECT 1 AS TOTAL,'
      
        #9'           (CASE WHEN RACK_STAT = '#39#48712#47001#39' THEN 1 ELSE 0 END) AS CN' +
        'T1,'
      #9#9#9'   (CASE WHEN RACK_STAT = '#39#51116#44256#39' THEN 1 ELSE 0 END) AS CNT2,'
      #9#9#9'   (CASE WHEN RACK_STAT = '#39#54028#47112#53944#39' THEN 1 ELSE 0 END) AS CNT3,'
      #9#9#9'   (CASE WHEN IN_USED = '#39'N'#39' THEN 1 ELSE 0 END) AS CNT4,'
      #9#9#9'   (CASE WHEN OT_USED = '#39'N'#39' THEN 1 ELSE 0 END) AS CNT5'
      #9#9'  FROM TT_RACK '
      #9#9' WHERE SC_HOGI = '#39'1'#39') AS TABLE1'
      'UNION'
      
        'SELECT 2 AS SC_HOGI, SUM(TOTAL) AS TOTAL, SUM(CNT1) AS CNT1, SUM' +
        '(CNT2) AS CNT2, SUM(CNT3) AS CNT3, SUM(CNT4) AS CNT4, SUM(CNT5) ' +
        'AS CNT5'
      '       FROM'
      #9'   (SELECT 1 AS TOTAL,'
      
        #9'           (CASE WHEN RACK_STAT = '#39#48712#47001#39' THEN 1 ELSE 0 END) AS CN' +
        'T1,'
      #9#9#9'   (CASE WHEN RACK_STAT = '#39#51116#44256#39' THEN 1 ELSE 0 END) AS CNT2,'
      #9#9#9'   (CASE WHEN RACK_STAT = '#39#54028#47112#53944#39' THEN 1 ELSE 0 END) AS CNT3,'
      #9#9#9'   (CASE WHEN IN_USED = '#39'N'#39' THEN 1 ELSE 0 END) AS CNT4,'
      #9#9#9'   (CASE WHEN OT_USED = '#39'N'#39' THEN 1 ELSE 0 END) AS CNT5'
      #9#9'  FROM TT_RACK '
      #9#9' WHERE SC_HOGI = '#39'2'#39') AS TABLE2'
      'UNION'
      
        'SELECT 3 AS SC_HOGI, SUM(TOTAL) AS TOTAL, SUM(CNT1) AS CNT1, SUM' +
        '(CNT2) AS CNT2, SUM(CNT3) AS CNT3, SUM(CNT4) AS CNT4, SUM(CNT5) ' +
        'AS CNT5'
      '       FROM'
      #9'   (SELECT 1 AS TOTAL,'
      
        #9'           (CASE WHEN RACK_STAT = '#39#48712#47001#39' THEN 1 ELSE 0 END) AS CN' +
        'T1,'
      #9#9#9'   (CASE WHEN RACK_STAT = '#39#51116#44256#39' THEN 1 ELSE 0 END) AS CNT2,'
      #9#9#9'   (CASE WHEN RACK_STAT = '#39#54028#47112#53944#39' THEN 1 ELSE 0 END) AS CNT3,'
      #9#9#9'   (CASE WHEN IN_USED = '#39'N'#39' THEN 1 ELSE 0 END) AS CNT4,'
      #9#9#9'   (CASE WHEN OT_USED = '#39'N'#39' THEN 1 ELSE 0 END) AS CNT5'
      #9#9'  FROM TT_RACK '
      #9#9' WHERE SC_HOGI = '#39'3'#39') AS TABLE2'
      '')
    Left = 1224
    Top = 968
  end
