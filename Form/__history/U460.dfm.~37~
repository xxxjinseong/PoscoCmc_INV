  object qryInfo: TADOQuery
    Connection = MainDm.MainDatabase
    CursorType = ctStatic
    CommandTimeout = 100
    Parameters = <>
    SQL.Strings = (
      ''
      
        'SELECT ORD_ALC_CODE, ORD_PROD_CODE1, ORD_PROD_CODE2, ORD_PROD_CO' +
        'DE3, ORD_PROD_CODE4, SUM(IN_CNT) AS IN_CNT, SUM(OUT_CNT) AS OUT_' +
        'CNT'
      '  FROM ('
      
        '  SELECT ORD_ALC_CODE, COUNT(ORD_ALC_CODE) AS IN_CNT , 0 AS OUT_' +
        'CNT,ORD_PROD_CODE1, ORD_PROD_CODE2, ORD_PROD_CODE3, ORD_PROD_COD' +
        'E4'
      #9'FROM TT_TRANS '
      #9'WHERE ORD_TYPE = '#39#49373#49328#51077#44256#39
      
        #9'GROUP BY ORD_ALC_CODE, ORD_PROD_CODE1, ORD_PROD_CODE2, ORD_PROD' +
        '_CODE3, ORD_PROD_CODE4'
      #9'UNION ALL'
      
        #9'SELECT ORD_ALC_CODE, 0 AS IN_CNT, COUNT(ORD_ALC_CODE) AS OUT_CN' +
        'T,ORD_PROD_CODE1, ORD_PROD_CODE2, ORD_PROD_CODE3, ORD_PROD_CODE4'
      #9'  FROM TT_TRANS '
      #9' WHERE ORD_TYPE IN ('#39#49436#50676#52636#44256#39', '#39#51648#51221#52636#44256#39', '#39#44596#44553#52636#44256#39')'
      
        #9'GROUP BY ORD_ALC_CODE, ORD_PROD_CODE1, ORD_PROD_CODE2, ORD_PROD' +
        '_CODE3, ORD_PROD_CODE4'
      
        #9') AS TOTAL GROUP BY ORD_ALC_CODE, ORD_PROD_CODE1, ORD_PROD_CODE' +
        '2, ORD_PROD_CODE3, ORD_PROD_CODE4'
      ''
      ''
      ''
      '')
    Left = 120
    Top = 280
  end
